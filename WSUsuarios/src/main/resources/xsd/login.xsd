<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://pagosmoviles.com/wsusuarios"
           targetNamespace="http://pagosmoviles.com/wsusuarios" elementFormDefault="qualified">
    <xs:element name="loginRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="username" type="xs:string"/>
                <xs:element name="password" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="loginResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="message" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>